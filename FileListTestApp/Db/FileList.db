#FileList
###################
record(waveform, "$(P)DIR")
{
   field(DTYP, "asynOctetWrite")
   field(INP, "@asyn($(READ),0,1)DIRBASE")
   field(FTVL, "CHAR")
   field(NELM, 256)
}

record(waveform, "$(P)FILES")
{
	field(FTVL, "CHAR")
	field(DTYP, "asynOctetRead")
    field(INP, "@asyn($(READ),0,1)JARR")	
	field(NELM, 16384)
	field(SCAN, "I/O Intr")
}

record(waveform, "$(P)SEARCH")
{
   field(DTYP, "asynOctetWrite")
   field(INP, "@asyn($(READ),0,1)SEARCH")
   field(FTVL, "CHAR")
   field(NELM, 256)
}

record(bo, "$(P)CASESENSITIVE")
{
	field(DTYP, "asynInt32")
	field(OUT, "@asyn($(READ),0,1)CASE")
	field(ZNAM, "Insensitive")
    field(ONAM, "Sensitive")
}	